{% comment %}theme-check-disable TemplateLength{% endcomment %}
{% if section.index > 2 or section.location == 'footer' %}
  <link
    rel="stylesheet"
    href="{{ 'component-product.css' | asset_url }}"
    media="print"
    onload="this.media='all'"
  >
  <link
    rel="stylesheet"
    href="{{ 'section-product.css' | asset_url }}"
    media="print"
    onload="this.media='all'"
  >
  <noscript>
    {{ 'component-product.css' | asset_url | stylesheet_tag }}
    {{ 'section-product.css' | asset_url | stylesheet_tag }}
  </noscript>
{% else %}
  {{ 'component-product.css' | asset_url | stylesheet_tag }}
  {{ 'section-product.css' | asset_url | stylesheet_tag }}
{% endif %}
{%- liquid
  assign product = all_products[section.settings.featured_product]

  if product.empty?
    assign section_onboarding = true
  endif

  assign current_variant = product.selected_or_first_available_variant

  unless section_onboarding
    assign price = current_variant.price
  else
    assign price = 1999
  endunless

  assign form_id = 'product-form-' | append: section.id
-%}
<section
  class="
    section
    section--padded
    section--{{ section.id }}
    section--product-featured
  "
  data-section-id="{{ section.id }}"
  data-section-type="product-featured"
  {% if section_onboarding %}
    data-section-onboarding="true"
  {% endif %}
  style="
    --space-top: {{ section.settings.padding_top }}px;
    --space-bottom: {{ section.settings.padding_bottom }}px
  "
>
  <product-single
    class="
      product-single
      product-single--featured
      product-single--{{ section.settings.content_align }}
      product-single--{{ product.id }}
      js-product-{{ product.id }}
    "
    data-product-id="{{ product.id }}"
  >
    <div class="container container--medium">
      <div class="o-layout o-layout--large u-flex u-flex--middle{% if section.settings.content_align == 'reverse' %} o-layout--reverse{% endif %}">
        <div class="o-layout__item u-1/1 u-1/2@tab">
          {% unless section_onboarding %}
            {%- liquid
              if section.settings.thumbnails_position == 'left'
                assign thumbnails_aside = true
              else
                assign thumbnails_aside = false
              endif
            -%}
            {% render 'product-gallery',
              product: product,
              section_id: section.id,
              container: 'small',
              show_arrows: section.settings.enable_gallery_arrows,
              image_ratio: section.settings.product_image_ratio,
              thumbnails_aside: thumbnails_aside,
              thumbnails_ratio: section.settings.thumbnails_ratio,
              thumbnails_show_border: section.settings.thumbnails_show_border,
              media_rounded_corners: section.settings.enable_product_image_rounded_corners,
              sizes: '(min-width: 1200px) 555px, (min-width: 740px) 50vw, calc(100vw - 36px)'
            %}
          {% else %}
            <div class="media-gallery__wrapper">
              <div
                class="
                  media-gallery__item
                  media-gallery__item--image
                "
              >
                <div class="o-ratio o-ratio--1:1">
                  <div class="o-ratio__content">
                    {{ 'product-1' | placeholder_svg_tag: 'placeholder-svg placeholder-svg--light' }}
                  </div>
                </div>
              </div>
            </div>
          {% endunless %}
        </div>

        <div class="o-layout__item u-1/1 u-1/2@tab">
          <div
            class="product-single__details js-animate-single"
            data-animation-displacement="0"
          >
            {% unless section_onboarding %}
              {%- liquid
                assign buy_buttons_present = false
                assign sizes_max_section_width = 1200
              -%}

              {%- for block in section.blocks -%}
                {%- liquid
                  if block.type == 'buy_buttons'
                    assign buy_buttons_present = true
                  endif

                  assign column_size_multiplier = 0.5
                  if block.type == 'countdown'
                    assign image_sizes = '(min-width: 516px) 480px, calc(100vw - 36px)'
                    assign large_image_sizes = '(min-width: ' | append: sizes_max_section_width | append: 'px) calc(' | append: sizes_max_section_width | append: 'px * ' | append: column_size_multiplier | append: '), (min-width: 767px) calc(100vw * ' | append: column_size_multiplier | append: ' - 126px), '
                    assign image_sizes = image_sizes | prepend: large_image_sizes
                  elsif block.type == 'image'
                    assign min_width = block.settings.image_width | plus: 1
                    assign max_width = block.settings.image_width
                    assign block_sizes = 'calc(100vw - 36px)'
                    if min_width > 480
                      assign block_sizes = block_sizes | prepend: '(min-width: 480px) 480px, '
                      if min_width > 540
                        assign max_width = 540
                        assign block_sizes = block_sizes | prepend: 'px, '
                        assign block_sizes = block_sizes | prepend: max_width
                        assign block_sizes = block_sizes | prepend: '(min-width: 767px) '
                      else
                        assign media_query = '(min-width: ' | append: min_width | append: 'px) ' | append: block.settings.image_width | append: 'px, '
                        assign block_sizes = block_sizes | prepend: media_query
                      endif
                    else
                      assign media_query = '(min-width: ' | append: min_width | append: 'px) ' | append: block.settings.image_width | append: 'px, '
                      assign block_sizes = block_sizes | prepend: media_query
                    endif
                  elsif block.type == 'profile'
                    assign image_sizes = 'calc(20vw - 36px)'
                    if block.settings.layout_mobile == 'mob-col-1'
                      assign image_sizes = 'calc(100vw - 36px)'
                    endif
                    if block.settings.layout_desktop == 'desk-col-1'
                      assign image_sizes = '(min-width: 516px) 480px, ' | append: image_sizes
                      assign large_image_sizes = '(min-width: ' | append: sizes_max_section_width | append: 'px) calc(' | append: sizes_max_section_width | append: 'px * ' | append: column_size_multiplier | append: '), (min-width: 767px) calc(100vw * ' | append: column_size_multiplier | append: ' - 126px), '
                      assign image_sizes = image_sizes | prepend: large_image_sizes
                    else
                      assign image_sizes = image_sizes | prepend: '(min-width: 516px) calc(0.2 * 480px), '
                      assign large_image_sizes = '(min-width: ' | append: sizes_max_section_width | append: 'px) calc(' | append: sizes_max_section_width | append: 'px * ' | append: column_size_multiplier | append: ' * 0.2), (min-width: 767px) calc((100vw * ' | append: column_size_multiplier | append: ' - 126px) / 5), '
                      assign image_sizes = image_sizes | prepend: large_image_sizes
                    endif
                  endif
                -%}
                {%- if block.type == 'title' -%}
                  <style type="text/css">
                    .section--{{ section.id }} .product-single__title {
                      font-size: {{ block.settings.title_size | times: 0.2 | times: 2 | plus: 1 }}rem;
                      line-height: calc(1.55 - (1.{{ block.settings.title_size }} * 0.3));
                    }
                    @media screen and (max-width: 767px) {
                      .section--{{ section.id }} .product-single__title {
                        font-size: {{ block.settings.title_size_mobile | times: 0.15 | times: 2 | plus: 1 }}rem;
                        line-height: calc(1.6 - (1.{{ block.settings.title_size_mobile }} * 0.3));
                      }
                    }
                  </style>
                {%- elsif block.type == 'vendor' -%}
                  <style type="text/css">
                    .section--{{ section.id }} .product-single__vendor {
                      font-size: {{ block.settings.vendor_size | times: 0.075 | times: 2 | plus: 0.75 }}rem;
                    }
                    @media screen and (max-width: 767px) {
                      .section--{{ section.id }} .product-single__vendor {
                        font-size: {{ block.settings.vendor_size | times: 0.07 | times: 2 | plus: 0.7 }}rem;
                      }
                    }
                  </style>
                {%- endif -%}

                {%- render 'pdp-blocks',
                  product_form_id: form_id,
                  current_block: block,
                  current_product: product,
                  current_variant: current_variant,
                  current_section: section,
                  image_sizes: image_sizes,
                  link_to_product: true,
                  heading_level: 2
                -%}
              {%- endfor -%}

              {%- unless buy_buttons_present -%}
                <product-form
                  {% unless settings.cart_type == 'page' %}
                    notify-on-add
                  {% else %}
                    redirect-on-add="{{ routes.cart_url }}"
                  {% endunless %}
                >
                  {% form 'product', product, class: 'product-form product-form--single js-product-form', id: form_id %}
                    <input
                      type="hidden"
                      name="id"
                      value="{{ current_variant.id }}"
                      {% unless settings.cart_type == 'page' %}
                        disabled
                      {% endunless %}
                    >
                    {% for variant in product.variants %}
                      <input
                        type="hidden"
                        id="quantity-{{ section.id }}-{{ variant.id }}"
                        data-qty="{{ variant.inventory_quantity }}"
                        disabled
                      >
                    {% endfor %}
                  {% endform %}
                </product-form>
              {%- endunless -%}
            {% else %}
              <div class="section__title section__title--onboarding js-animate-single" data-animation-displacement="5">
                <p
                  class="section__title-desc product-single__title-desc js-animate-single"
                  data-animation-displacement="0"
                  data-animation-delay="0.1"
                >
                  {{ 'homepage.onboarding.product_vendor' | t }}
                </p>
                <h2 class="product-single__title">
                  <a href="{{ product.url }}">{{ 'homepage.onboarding.product_title' | t }}</a>
                </h2>
              </div>

              <form class="product-form js-product-form" id="js-product-form-{{ product.id }}">
                <div class="product-single__box__block product-single__box__block--m-12 product-single__box__block--price js-product-price">
                  <div class="price js-product-price-text">
                    <span class="u-hidden-visually">{{ 'products.general.regular_price' | t }}</span>
                    <span class="price__text js-product-price-number">
                      <span class="price__number"
                        ><span class="money">{{ price | money }}</span></span
                      >
                    </span>
                  </div>
                </div>

                <div class="product-form__qty">
                  <label
                    for="Quantity-{{ section.id }}"
                    class="quantity-selector f-family--{{ settings.type_product_form_headings_style }} f-caps--{{ settings.type_product_form_headings_capitalize }} f-space--{{ settings.type_product_form_headings_letterspace }}"
                  >
                    {{- 'products.product.quantity' | t -}}
                  </label>
                  <div class="product-form__qty-input">
                    <quantity-input>
                      <button minus type="button" aria-label="{{ 'cart.general.reduce_quantity' | t }}">
                        <span aria-hidden="true">&minus;</span>
                      </button>
                      {% comment %} TODO: quantity_rule {% endcomment %}
                      <input
                        id="Quantity-{{ section.id }}"
                        type="number"
                        value="1"
                        min="1"
                        step="1"
                        name="quantity"
                        autocomplete="off"
                      >
                      <button plus type="button" aria-label="{{ 'cart.general.increase_quantity' | t }}">
                        <span aria-hidden="true">+</span>
                      </button>
                    </quantity-input>
                  </div>
                </div>

                <div class="product-form__add">
                  <button
                    type="submit"
                    name="add"
                    class="c-btn c-btn--full c-btn--primary product-form__add-btn js-product-add"
                    disabled
                  >
                    <span id="AddToCartText" class="js-product-add-text">{{ 'products.product.add_to_cart' | t }}</span>
                  </button>
                </div>
              </form>
            {% endunless %}
          </div>
        </div>
      </div>
    </div>
  </product-single>
  {{ modals }}
  <script src="{{ 'product-single.js' | asset_url }}" type="module"></script>
</section>

{% unless product == empty %}
  <script type="application/json" id="ModelJson-{{ section.id }}">
    {{ product.media | where: 'media_type', 'model' | json }}
  </script>
{% endunless %}

{%- if request.design_mode -%}
  <script src="{{ 'variant-select-components.js' | asset_url }}" type="module"></script>
  <script src="{{ 'product-interactive-options.js' | asset_url }}" type="module"></script>
{%- endif -%}

{%- liquid
  if product.selected_or_first_available_variant.featured_media
    assign seo_media = product.selected_or_first_available_variant.featured_media
  else
    assign seo_media = product.featured_media
  endif
-%}
<script type="application/ld+json">
  {
    "@context": "http://schema.org/",
    "@type": "Product",
    "name": {{ product.title | json }},
    "url": {{ request.origin | append: product.url | json }},
    {% if seo_media -%}
      "image": [
        {{ seo_media | image_url: width: seo_media.preview_image.width | prepend: "https:" | json }}
      ],
    {%- endif %}
    "description": {{ product.description | strip_html | json }},
    {% if product.selected_or_first_available_variant.sku != blank -%}
      "sku": {{ product.selected_or_first_available_variant.sku | json }},
    {%- endif %}
    "brand": {
      "@type": "Brand",
      "name": {{ product.vendor | json }}
    },
    "offers": [
      {%- for variant in product.variants -%}
        {
          "@type" : "Offer",
          {%- if variant.sku != blank -%}
            "sku": {{ variant.sku | json }},
          {%- endif -%}
          {%- if variant.barcode.size == 12 -%}
              "gtin12": {{ variant.barcode }},
          {%- endif -%}
          {%- if variant.barcode.size == 13 -%}
            "gtin13": {{ variant.barcode }},
          {%- endif -%}
          {%- if variant.barcode.size == 14 -%}
            "gtin14": {{ variant.barcode }},
          {%- endif -%}
          "availability" : "http://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
          "price" : {{ variant.price | divided_by: 100.00 | json }},
          "priceCurrency" : {{ cart.currency.iso_code | json }},
          "url" : {{ request.origin | append: variant.url | json }}
        }{% unless forloop.last %},{% endunless %}
      {%- endfor -%}
    ]
  }
</script>

